<Project>

	<PropertyGroup>
		<OutputType>Exe</OutputType>
		<RootNamespace>LSEG.Eta.ValueAdd.WatchlistConsumer</RootNamespace>
		<Nullable>enable</Nullable>
		<AssemblyName>WatchlistConsumer</AssemblyName>
		<ImplicitUsings>enable</ImplicitUsings>
		<ExamplePath>$(ProjectDir)..\..\..\Applications\Examples\WatchlistConsumer\</ExamplePath>
		<BaseOutputPath>$(ProjectDir)..\..\..\Executables\QATools\$(MSBuildProjectName)</BaseOutputPath>
	</PropertyGroup>

	<ItemGroup>
		<ProjectReference Include="..\..\..\Src\Core\Core.csproj" />
		<ProjectReference Include="..\..\..\Src\ValueAdd\ValueAdd.csproj" />
		<ProjectReference Include="..\..\..\Applications\Examples\EtaAppCommon\EtaAppCommon.csproj" />
	</ItemGroup>
	
	<ItemGroup>
		<QAToolFiles Include="*.cs"/>
	</ItemGroup>
	<ItemGroup>
		<Compile Include="$(ExamplePath)\*.cs"/>
		<Compile Remove="@(QAToolFiles ->'$(ExamplePath)\%(Filename)%(Extension)')"/>
	</ItemGroup>
	
</Project>
